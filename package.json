    "twilio": "5.0.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "copyfiles": "^2.4.1",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "twilio-run": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "22"
  }
}
